#include<bits/stdc++.h> 
using namespace std; 

// } Driver Code Ends
//User function Template for C++
class Solution
{
public:
    void printTillN(int N)
    {
        if(N==0){
            return ;
        }
        printTillN(N-1);
        // Write Your Codie here
        cout<<N<<" ";
    }
};

